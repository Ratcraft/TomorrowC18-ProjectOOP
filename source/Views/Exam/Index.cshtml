@model Dictionary<String, List<Models.Exam>>
@{
    ViewData["Title"] = "Index";
}

<h1>Your exams</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => Model)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (KeyValuePair<String, List<Exam>> item in Model)
        {
            <p>Subject : @item.Key</p>
            @foreach (Exam ex in item.Value)
            {
                <p>For the test you had on the @ex.Date, you have   @ex.Grade /20</p>
            }
        }
    </tbody>
</table>

